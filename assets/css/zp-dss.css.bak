@media only screen and (max-width: 767px) {
  [class*="mobile hidden"],
  [class*="tablet only"]:not(.mobile),
  [class*="computer only"]:not(.mobile),
  [class*="large monitor only"]:not(.mobile),
  [class*="widescreen monitor only"]:not(.mobile),
  [class*="or lower hidden"] {
    display: none !important;
  }

  .ui.text.container{
      background-position-x: -180px!important;
  }
  
}

@media only screen and (min-width: 360px) and (max-width: 700px) {

    .ui.visible.uncover.sidebar{
        background: #003948;
    }

    .sidebar .sidebar-logocompany{
        width: 130px!important;
    }

    .ui.sidebar.menu .item{
        color: #ffffff;
        float: left;
        width: 100%;
    }

    .ui.sidebar.menu :nth-child(1){
        width: 70%;
    }

    .ui.sidebar.menu :nth-child(10){
        margin-top: 270px;
    }

    .ui.sidebar.menu .item.zp-dss-language{
        float: right;
        width: auto;
    }

    .ui.fixed.menu {
        display: none !important;
    }
    .secondary.pointing.menu .item,
    .secondary.pointing.menu .right.item,
    .secondary.pointing.menu .menu {
        display: none;
    }
    .secondary.pointing.menu .header.item,
    .secondary.pointing.menu .toc.item {
        display: block;
    }

    .secondary.pointing.menu .toc.item{
        padding: 10px;
        position: absolute;
        top: 15px;
        font-size: 20px;
    }

    .ui.secondary.pointing.menu .header.item{
        margin: 0px auto;
    }

    .logo-company{
        min-width: 100px;
    }

    .masthead.segment {
        min-height: 350px;
    }
    .masthead h1.ui.header {
        font-size: 2em;
        margin-top: 1.5em;
    }
    .masthead h2 {
        margin-top: 0.5em;
        font-size: 1.5em;
    }

    .ui.text.container{
        margin: 0px!important;
        background-position-x: -500px!important;
        background-position-y: 0px!important;
        background-size: 400%!important;
    }

    .zp-dss-imagegsk{
        right: 0;
        left: 0;
        width: 60px;
        margin: 0px auto;
    }

    .masthead h1.ui.welcomeback {
        font-size: 18px;
        margin: 0px auto;
        left: 33%;
        position: absolute;
        top: 190px;
        padding-top: 0px;
    }

    .masthead h1.ui.header{
        font-size: 35px;
        margin: 0px auto;
        left: 29%;
        position: absolute;
        top: 215px;
        padding-top: 0px;
    }

    .ui.table:not(.unstackable) thead{
        display: none;
    }

    .ui.vertical.footer.segment{
        padding-bottom: 0px;
    }

    .ui.container.zp-dss-footer{
        margin-left: 0px!important;
        margin-right: 0px!important;
    }
    
    .zp-dss-footer{
        text-align: center;
        max-height: 100%;
    }

    .zp-dss-copyright{
        text-align: center;
    }

}


@media only screen and (min-width: 768px) {

    .ui.text.container{
